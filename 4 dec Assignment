class vehicle:
   color = "white"
   def __init__(self, name, max_speed, mileage)
       self.name = name
       self.max_speed = max_speed
       self.mileage = mileage
       
class Bus(vehicle):
   pass
class car(vehicle)
   pass
   
x = Bus("gds",839,3424)
print("color:", x.color, "vehicle name:" x.name, "speed:, x.max_speed, "mileage:", x.mileage)


y = car("jdf",43,24)
print("color:" x.color, "vehicle name:" x.name,"speed:",x.max_speed, "mileage:", x.mileage)



