Ans 8
class vehicle:
   color = "white"
   def __init__(self, name, max_speed, mileage)
       self.name = name
       self.max_speed = max_speed
       self.mileage = mileage
       
class Bus(vehicle):
   pass
   
x = Bus("sdf",23,32)
print(isinstance(x,vehicle)
  
